    <div class="row align-items-center justify-content-center">
      <div style="text-align: center;" data-aos="fade-up" data-aos-delay="400">
        <h1 class="mb-4">Sami-Room</h1>
        <p class="mb-5">העתיד הבא של דירות הסטודנטים כבר כאן</p>
        <p><a href="#" class="btn btn-primary px-5 py-3">החל חיפוש</a></p>
        <!-- Search form -->
        <form>
          <tr>
            <div class="md-form mt-0">
              <input class="form-control text-white" style="font-size: x-large;" type="text" placeholder="Search"
                aria-label="Search">
            </div>
          </tr>
          <tr>
            <td>
              <select searchable="Search here..">
                <option value="" disabled selected>מספר חדרים</option>
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="3">4</option>
                <option value="3">5</option>
              </select>
            </td>
            <td>
              <select searchable="Search here..">
                <option value="" disabled selected>עיר מגורים</option>
                <option value="ashdod">אשדוד</option>
                <option value="bearsheva">באר שבע</option>
              </select>
            </td>
            <td>
              <select searchable="Search here..">
                <option value="" disabled selected>...מיין לפי</option>
                <option value="1">מחיר - מהנמוך לגבוה</option>
                <option value="2">מחיר - מהגבוה לנמוך</option>
                <option value="3">תאריך סיום קרוב</option>
                <option value="3">דירוג - מהגבוה לנמוך</option>
              </select>
            </td>
            <td>
              <input type="date" style="height: 26px;" id="startDate">
              <label class="text-white">:תאריך כניסה</label>
            </td>
          </tr>
        </form>
      </div>
    </div>
  </div>

  <!-------------------------------------- all Apartments --------------------------------------------------->
  <div class="site-section">
    <div class="container">
      <div class="row">
        <form action="http://localhost:3000/details" method="post" style="position: relative; margin: auto;">
          <ul>
            <% for(var key in apartments) {  %>
            <div class="img_wrap">
              <a href='/details'><input type="image" class="img_wrap" name='<%=apartments[key][1] %>' 
                src='<%=apartments[key][3]%>/apart1.jpg' width="250" height="150" /></a>
              <div class="imgText"><%=apartments[key][0] %>, <%=apartments[key][9] %></div>
            </div>
            <% } %>

          </ul>
        </form>
      </div>
    </div>

    <div class="container mt-5" data-aos="fade-up">
      <div class="row">
        <div class="col-md-12 text-center">
          <div class="site-block-27">
            <ul>
              <li><a href="#">&lt;</a></li>
              <li class="active"><span>1</span></li>
              <li><a href="#">2</a></li>
