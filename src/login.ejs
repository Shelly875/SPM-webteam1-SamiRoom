          <div class="form">
            <form class="register-form" id="register-form" action="/" method="POST">
              <input type="text" id="fname" name="user[fname]" placeholder="first name"/>
              <input type="text"  id="lname" name="user[lname]" placeholder="last name"/>
              <input type="text" id="regid" name="user[regid]" placeholder="id"/>
              <input type="tel" id="phone" name="user[phone]" pattern="[0-9]{2-3}-[0-9]{7}" placeholder="phone number"/>
              <input type="password" id="regpass" name="user[regpass]" placeholder="password"/>
              <input type="text" id="city" name="user[city]" placeholder="city"/>
              <input type="text" id="address" name="user[address]" placeholder="address"/>
              <label for="birthday">birthday</label>
              <input type="date" id="birthday" name="user[birthday]" placeholder="birthday" class="textbox-n"/>
              <input type="email" id="email" name="user[email]" placeholder="email"/>
              <label for="male">
              <input type="radio" id="gender" name="user[gender]" value="male" checked>
              male
              </label>
              <input type="radio" id="gender" name="user[gender]" value="female">
              <label for="female">Female</label>
              
              <div class="userType" >
                <select class="form-control" id="userType" name="user[userType]" placeholder="user type">
                  <option value="student">student</option>
                  <option value="landlord">landlord</option>
                </select>
              </div>
              <div class="form-group" id="studentdiv">
                <input type="text" id="regid" name="user[regidcard]" placeholder="student id card"/>
              </div>
              <ul>
               <input type="submit" class="btn btn-primary" value="שלח">
              </ul>
              <p class="message">Already registered? <a href="#">Sign In</a></p>
              <div id="reg-error"></div>
            </form>
            <form class="login-form" id="login-form" action="/login" method="POST">
              <input type="text" id="userid" name="user[id]" placeholder="ID"/>
              <input type="password" id="password" name="user[pass]" placeholder="password"/>
              <input type="submit" class="btn btn-primary" value="שלח">
              <p class="message">Not registered? <a href="#">Create an account</a></p>
              <div id="login-error">
              </div>
            </form>
          </div>
 
